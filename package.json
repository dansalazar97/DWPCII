{
  "name": "dwpcii1",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "NODE_ENV=development DEBUG=dwpcii:server babel-watch ./server/bin/www",
    "devc": "NODE_ENV=development webpack serve --config ./webpack.dev.config.js --mode development",
    "devb": "DEBUG=dwpcii1:server && babel-watch ./server/bin/www",
    "cleans": "rm -rf ./dist",
    "cleanc": "rm -f ./public/bundle.js ./public/styles/app.css",
    "b:s": "npm run cleans && babel ./server --out-dir dist --copy-files",
    "b:c": "npm run cleanc && webpack --config ./webpack.config.js --mode production",
    "build": "npm run b:c && npm run b:s",
    "clean:logs": "rm -rf ./server/logs/*.log"
  },
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "hbs": "^4.2.0",
    "http-errors": "^2.0.0",
    "materialize-css": "^1.0.0",
    "morgan": "^1.10.0",
    "node-modules": "^1.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.4",
    "winston": "^3.11.0"
  }
}
